<h1>出品一覧</h1>

<table>
<thead>
    <tr>
        <th>Name</th>
        <th>Price</th>
        <th colspan="3"></th>
    </tr>
</thead>

<p><a href= 'http://localhost:3000/put_entry'>新規作成</a></p>

    <tbody>
        <% @products.each do |product| %>
            <tr>
                <td><%= product.name %></td>
                <td><%= product.price %></td>
            </tr>
        <% end %>
    </tbody>


